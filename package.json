{
	"name": "eleventy-web-starter",
	"version": "1.7.0",
	"description": "Eleventy Web Starter is a lightweight Eleventy boilerplate utilising, ESBuild Tailwind and PostCSS",
	"scripts": {
		"clean": "del dist",
		"dev:postcss": "postcss src/assets/css/*.css -o dist/assets/main.min.css --watch --verbose",
		"dev:scripts": "esbuild src/assets/scripts/main.js --bundle --watch --outfile=dist/assets/main.min.js",
		"dev:11ty": "eleventy --serve --watch",
		"build:postcss": "NODE_ENV=production postcss src/assets/css/main.css -o src/_includes/main.min.css",
		"build:11ty": "npx cross-env ELEVENTY_ENV=production eleventy",
		"build:scripts": "esbuild src/assets/scripts/main.js --bundle --minify --outfile=dist/assets/main.min.js",
		"start": "run-p dev:*",
		"build": "run-s clean build:*",
		"debug": "set DEBUG=Eleventy* & npx @11ty/eleventy",
		"debug-perf": "set DEBUG=Eleventy:Benchmark* & npx @11ty/eleventy"
	},
	"author": "Chris Collins",
	"license": "ISC",
	"dependencies": {
		"@11ty/eleventy-plugin-rss": "^1.1.1",
		"@11ty/eleventy-plugin-syntaxhighlight": "^3.1.1",
		"@tailwindcss/aspect-ratio": "^0.2.1",
		"@tailwindcss/forms": "^0.3.3",
		"@tailwindcss/typography": "^0.4.1",
		"alpinejs": "^3.2.2",
		"eleventy-plugin-embed-everything": "^1.13.2",
		"lite-youtube-embed": "^0.2.0",
		"lodash": "^4.17.21",
		"markdown-it": "^12.0.6",
		"markdown-it-emoji": "^2.0.0",
		"tailwindcss": "^2.2.6"
	},
	"devDependencies": {
		"@11ty/eleventy": "^1.0.0",
		"@11ty/eleventy-img": "^1.0.0",
		"@pkvach/eleventy-plugin-pwa": "^1.3.0",
		"autoprefixer": "^10.3.1",
		"axios": "^0.21.1",
		"calendar-link": "^2.1.1",
		"colorthief": "^2.3.2",
		"date-fns": "^2.22.1",
		"del-cli": "^3.0.1",
		"dotenv": "^8.6.0",
		"esbuild": "^0.12.15",
		"html-minifier": "^4.0.0",
		"image-size": "^1.0.1",
		"luxon": "^2.0.1",
		"moment": "^2.29.1",
		"netlify-plugin-submit-sitemap": "^0.2.3",
		"npm-run-all": "^4.1.5",
		"outdent": "^0.8.0",
		"postcss-cli": "^8.3.1",
		"postcss-import": "^14.0.2",
		"prettier": "^2.5.1",
		"sharp": "^0.28.3",
		"svg-sprite": "^1.5.0"
	}
}
