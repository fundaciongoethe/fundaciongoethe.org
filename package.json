{
  "name": "fundaciongoethe",
  "version": "2.1.15",
  "description": "fundaciongoethe uses ESBuild Tailwind and PostCSS",
  "engines": {
    "node": ">=20.x.x"
  },
  "scripts": {
    "clean": "del dist",
    "dev:postcss": "postcss src/assets/css/*.css -o dist/assets/main.min.css --watch --verbose",
    "dev:scripts": "esbuild src/assets/scripts/main.js src/assets/scripts/validate.js src/assets/scripts/audio.js src/assets/scripts/snow-fall.js --bundle --watch --outdir=dist/assets",
    "dev:11ty": "eleventy --serve --watch --incremental",
    "build:postcss": "npx cross-env NODE_ENV=production postcss src/assets/css/main.css -o src/_includes/main.min.css",
    "build:11ty": "npx cross-env ELEVENTY_ENV=production eleventy",
    "build:scripts": "esbuild src/assets/scripts/main.js src/assets/scripts/validate.js src/assets/scripts/audio.js src/assets/scripts/snow-fall.js --bundle --minify --outdir=dist/assets",
    "build:sw": "workbox generateSW",
    "start": "run-p dev:*",
    "build": "run-s clean build:*",
    "debug": "set DEBUG=Eleventy* & npx @11ty/eleventy",
    "debug-perf": "set DEBUG=Eleventy:Benchmark* & npx @11ty/eleventy"
  },
  "author": "Lene Saile",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.4",
    "alpinejs": "^3.10.3",
    "eleventy-plugin-embed-everything": "^1.14.1",
    "lite-youtube-embed": "^0.3.0",
    "lodash": "^4.17.21",
    "markdown-it": "^14.0.0",
    "markdown-it-emoji": "^3.0.0",
    "tailwindcss": "^3.1.8",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^4.0.2",
    "autoprefixer": "^10.4.8",
    "calendar-link": "^2.2.0",
    "clean-css": "^5.3.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.2",
    "del-cli": "^5.0.0",
    "dotenv": "^16.0.2",
    "esbuild": "^0.20.2",
    "glob": "^8.0.3",
    "html-minifier": "^4.0.0",
    "luxon": "^3.1.1",
    "moment": "^2.29.4",
    "netlify-plugin-cache": "^1.0.3",
    "netlify-plugin-submit-sitemap": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "outdent": "^0.8.0",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "prettier": "^3.0.3",
    "slugify": "^1.6.5",
    "svg-sprite": "^2.0.2",
    "vinyl": "^3.0.0",
    "workbox-cli": "^7.0.0"
  }
}
