{
  "name": "fundaciongoethe",
  "version": "2.2.0",
  "description": "fundaciongoethe.org",
  "engines": {
    "node": ">=20.x.x"
  },
  "scripts": {
    "clean": "del dist",
    "dev:postcss": "postcss src/assets/css/*.css -o dist/assets/main.min.css --watch --verbose",
    "dev:scripts": "esbuild src/assets/scripts/*.js --bundle --watch --outdir=dist/assets",
    "dev:11ty": "eleventy --serve --watch --incremental --quiet",
    "build:postcss": "npx cross-env NODE_ENV=production postcss src/assets/css/main.css -o src/_includes/main.min.css",
    "build:11ty": "npx cross-env ELEVENTY_ENV=production eleventy",
    "build:scripts": "esbuild src/assets/scripts/*.js --minify --bundle --keep-names --sourcemap --outdir=dist/assets",
    "build:sw": "cp src/assets/scripts/sw.js dist/sw.js",
    "start": "run-p dev:*",
    "build": "run-s clean build:*",
    "debug": "set DEBUG=Eleventy* & npx @11ty/eleventy",
    "debug-perf": "set DEBUG=Eleventy:Benchmark* & npx @11ty/eleventy"
  },
  "author": "Lene Saile",
  "dependencies": {
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "alpinejs": "^3.14.9",
    "eleventy-plugin-embed-everything": "^1.21.0",
    "lite-youtube-embed": "^0.3.3",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "markdown-it-emoji": "^3.0.0",
    "tailwindcss": "^3.4.17",
    "validator": "^13.15.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "^6.0.3",
    "autoprefixer": "^10.4.21",
    "calendar-link": "^2.8.0",
    "clean-css": "^5.3.3",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "del-cli": "^6.0.0",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.4",
    "glob": "^8.0.3",
    "html-minifier-terser": "^7.2.0",
    "luxon": "^3.6.1",
    "moment": "^2.30.1",
    "netlify-plugin-cache": "^1.0.3",
    "netlify-plugin-submit-sitemap": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "outdent": "^0.8.0",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-jinja-template": "^2.1.0",
    "slugify": "^1.6.6",
    "svg-sprite": "^2.0.4",
    "vinyl": "^3.0.0"
  }
}
