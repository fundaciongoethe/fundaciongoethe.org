{
  "name": "fundaciongoethe",
  "version": "2.1.1",
  "description": "fundaciongoethe uses ESBuild Tailwind and PostCSS",
  "scripts": {
    "clean": "del dist",
    "dev:postcss": "postcss src/assets/css/*.css -o dist/assets/main.min.css --watch --verbose",
    "dev:scripts": "esbuild src/assets/scripts/main.js --bundle --watch --outfile=dist/assets/main.min.js",
    "dev:11ty": "eleventy --serve --watch",
    "build:postcss": "npx cross-env NODE_ENV=production postcss src/assets/css/main.css -o src/_includes/main.min.css",
    "build:11ty": "npx cross-env ELEVENTY_ENV=production eleventy",
    "build:scripts": "esbuild src/assets/scripts/main.js --bundle --minify --outfile=dist/assets/main.min.js",
    "start": "run-p dev:*",
    "build": "run-s clean build:*",
    "debug": "set DEBUG=Eleventy* & npx @11ty/eleventy",
    "debug-perf": "set DEBUG=Eleventy:Benchmark* & npx @11ty/eleventy"
  },
  "author": "Lene Saile",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "alpinejs": "^3.2.2",
    "eleventy-plugin-embed-everything": "^1.13.2",
    "lite-youtube-embed": "^0.2.0",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.0",
    "markdown-it-emoji": "^2.0.0",
    "tailwindcss": "^3.0.24",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0-canary.11",
    "@11ty/eleventy-img": "^2.0.0",
    "autoprefixer": "^10.3.1",
    "calendar-link": "^2.1.1",
    "clean-css": "^5.3.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.22.1",
    "del-cli": "^4.0.1",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.36",
    "glob": "^8.0.3",
    "html-minifier": "^4.0.0",
    "luxon": "^2.0.1",
    "moment": "^2.29.1",
    "netlify-plugin-11ty": "^1.1.0",
    "netlify-plugin-submit-sitemap": "^0.3.0",
    "npm-run-all": "^4.1.5",
    "outdent": "^0.8.0",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "slugify": "^1.6.5",
    "svg-sprite": "^1.5.4",
    "vinyl": "^2.2.1"
  }
}
