{
	"name": "sr-11ty-tailwind-alpine-template",
	"version": "1.0.0",
	"description": "Template for new projects with 11ty, TailwindCSS, TailwindUI, and Alpine.js",
	"author": "Shane Robinson",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/shanerobinson/sr-11ty-tailwind-alpine-template/issues"
	},
	"homepage": "https://github.com/shanerobinson/sr-11ty-tailwind-alpine-template#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/shanerobinson/sr-11ty-tailwind-alpine-template.git"
	},
	"keywords": [
		"11ty",
		"TailwindCSS",
		"TailwindUI",
		"Alpine.js",
		"webpack",
		"PostCSS",
		"CSSnano"
	],
	"scripts": {
		"clean": "npx rimraf dist",
		"dev:postcss": "postcss src/assets/css/main.css -o dist/assets/main.min.css --watch --verbose",
		"dev:scripts": "webpack --watch --config webpack.config.js",
		"dev:11ty": "eleventy --serve --watch",
		"build:postcss": "npx cross-env NODE_ENV=production postcss src/assets/css/main.css -o src/_includes/main.min.css",
		"build:scripts": "npx cross-env LEVENTY_ENV=production webpack -p --config webpack.config.js",
		"build:11ty": "npx cross-env ELEVENTY_ENV=production eleventy",
		"dev": "run-p dev:* --print-label",
		"build": "run-s clean build:* --print-label",
		"debug": "set DEBUG=Eleventy* & npx @11ty/eleventy",
		"debug-perf": "set DEBUG=Eleventy:Benchmark* & npx @11ty/eleventy"
	},
	"dependencies": {
		"@11ty/eleventy": "^0.12.1",
		"@11ty/eleventy-img": "^0.8.3",
		"@11ty/eleventy-navigation": "^0.2.0",
		"@11ty/eleventy-plugin-rss": "^1.1.1",
		"@11ty/eleventy-plugin-syntaxhighlight": "^3.1.0",
		"@tailwindcss/aspect-ratio": "^0.2.0",
		"@tailwindcss/forms": "^0.3.2",
		"@tailwindcss/typography": "^0.4.0",
		"alpinejs": "^2.8.2",
		"eleventy-plugin-embed-everything": "^1.13.1",
		"lite-youtube-embed": "^0.2.0",
		"markdown-it": "^12.0.6",
		"markdown-it-emoji": "^2.0.0",
		"tailwindcss": "^2.1.2"
	},
	"devDependencies": {
		"@babel/core": "^7.14.2",
		"@babel/preset-env": "^7.14.2",
		"autoprefixer": "^10.2.5",
		"axios": "^0.21.1",
		"babel-loader": "^8.2.2",
		"calendar-link": "^2.1.1",
		"clean-css": "^5.1.2",
		"cssnano": "^4.1.11",
		"date-fns": "^2.21.3",
		"dotenv": "^8.6.0",
		"eslint": "^7.28.0",
		"html-minifier": "^4.0.0",
		"moment": "^2.29.1",
		"npm-run-all": "^4.1.5",
		"outdent": "^0.8.0",
		"postcss": "^8.2.15",
		"postcss-cli": "^8.3.1",
		"postcss-import": "^12.0.1",
		"postcss-nested": "^4.2.3",
		"rimraf": "^3.0.2",
		"slugify": "^1.5.3",
		"svg-sprite": "^1.5.0",
		"terser-webpack-plugin": "^4.2.3",
		"webpack": "^4.46.0",
		"webpack-cli": "^3.3.12"
	}
}
